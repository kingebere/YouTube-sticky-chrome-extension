{
    "name": "YouTube Sticky",
    "description": "Youtube Sticky",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage"],
      "content_scripts":[{
        "matches": ["https://*.youtube.com/*"],
        "js":["whatsapp.js"]
    }]
  }